[
    {
        "run_name": "QA_Test_cache",
        "task_type": "QuestionAnswering",
        "device": "cuda",
        "dataset":{
            "url": {
                "train":"https://rajpurkar.github.io/SQuAD-explorer/dataset/train-v2.0.json",
                "test":"https://rajpurkar.github.io/SQuAD-explorer/dataset/dev-v2.0.json"
            },
            "download": false
        },
        "train":
            {
                "model_type": "QAModel",
                "model_names": [
                    "bert-base-cased",
                    "roberta-base",
                    "albert-base-v2"
                ],
                "feature_params":{
                    "load_from_cache_train":true,
                    "load_from_cache_test":false,
                    "max_seq_length":384,
                    "doc_stride":128,
                    "max_query_length":64
                },
                "train_params":{
                    "batch_size":2,
                    "train_epochs":1,
                    "subset_samples":0.05,
                    "gradient_accum_steps":1,
                    "warm_up_steps":0,
                    "optimizer":{
                        "name":"Adam",
                        "lr":3e-05
                    }
                },
                "eval_params":{
                    "batch_size":2,
                    "n_best":20,
                    "subset_samples":0.1,
                    "max_answer_length":30,
                    "do_lower_case":false
                }
            }
    }
]